{
    "name": "armosam/theraconnect",
    "description": "Thera Connect web application",
    "keywords": [
        "yii2",
        "framework",
        "advanced",
        "improved",
        "application template",
        "armosam"
    ],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "tutorial": "http://www.freetuts.org/tutorial/view?id=6",
        "source": "https://github.com/armosam/theraconnect"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "ext-pdo": "*",
        "ext-imagick": "*",
        "ext-fileinfo": "*",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "~2.0.10",
        "yiisoft/yii2-bootstrap4": "~2.0.7",
        "yiisoft/yii2-swiftmailer": "~2.1.2",
        "yiisoft/yii2-imagine": "~2.2.0",
        "yiisoft/yii2-queue": "~2.3.0",
        "yiisoft/yii2-authclient": "~2.2.0",
        "nenad/yii2-password-strength": "~2.3.0",
        "mihaildev/yii2-ckeditor": "~1.0.1",
        "floor12/yii2-metamaster": "~1.1.5",
        "nterms/yii2-pagesize-widget": "~2.0.1",
        "lajax/yii2-language-picker": "~1.2.1",
        "geoip2/geoip2": "~2.9.0",
        "dmstr/yii2-adminlte-asset": "~2.6.0",
        "borales/yii2-phone-input": "~0.3.0",
        "philippfrenzel/yii2fullcalendar": "3.9.0",
        "kartik-v/yii2-widgets": "~3.4.0",
        "kartik-v/yii2-date-range": "~1.7.1",
        "kartik-v/yii2-editable": "~1.7.8",
        "kartik-v/yii2-mpdf": "~1.0.5",
        "kartik-v/yii2-datecontrol": "~1.9.7",
        "kartik-v/yii2-builder": "~1.6.7",
        "twilio/sdk": "~5.13.3",
        "sendgrid/sendgrid": "~5.4",
        "mikehaertl/php-pdftk": "~0.9.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "yiisoft/yii2-debug": "~2.1.13",
        "yiisoft/yii2-gii": "~2.1.4",
        "yiisoft/yii2-faker": "~2.0.4",
        "codeception/codeception": "~4.1.3",
        "codeception/module-phpbrowser": "~1.0.0",
        "codeception/module-webdriver": "~1.0.5",
        "codeception/module-filesystem": "~1.0.2",
        "codeception/module-asserts": "~1.1.1",
        "codeception/module-yii2": "~1.1.0",
        "codeception/module-rest": "~1.2.0",
        "codeception/specify": "~1.3.0",
        "codeception/verify": "~1.3.0",
        "symfony/browser-kit": "~v5.0.5",
        "symfony/var-dumper": "~v5.0.5",
        "se/selenium-server-standalone": "~3.141.59"
    },
    "config": {
        "vendor-dir": "_protected/vendor",
        "process-timeout": 1800,
        "preferred-install": "dist",
        "fxp-asset": {
            "enabled": false,
            "installer-paths": {
                "npm-asset-library": "_protected/vendor/npm",
                "bower-asset-library": "_protected/vendor/bower"
            }
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "packagist": true,
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}

